{
  "extends": [
    "next",
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      0,
      {
        "extensions": [
          ".tsx"
        ]
      }
    ],
    "react/prop-types":[0],
    "react/jsx-props-no-spreading":[0],
    "import/extensions":[{ "tsx": "never", "ts": "never" }]
  }
}